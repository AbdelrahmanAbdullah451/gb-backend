@model IEnumerable<gb_raw.Models.Exam>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            Professor Name
        </th>
        <th>
            Exam Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.SysUser.userName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.examName)
    </td>
    <td>
        @Html.ActionLink("TakeExam", "Edit", new { id = item.examID })
    </td>
</tr>
}

</table>
